{
  "name": "amitakitchen",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db": "drizzle-kit generate",
    "local": "wrangler d1 migrations apply amitakitchen --local",
    "prod": "wrangler d1 migrations apply amitakitchen --remote",
    "pages:build": "npx @cloudflare/next-on-pages",
    "pages:dev": "npx pages:build && npm wrangler pages dev",
    "pages:deploy:preview": "npm pages:build && npm wrangler pages deploy --branch preview",
    "preview": "npx @cloudflare/next-on-pages --watch && wrangler pages dev --live-reload",
    "deploy": "npm run pages:build && wrangler pages publish --branch=master"
  },
  "dependencies": {
    "@next/third-parties": "^15.0.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.36.0",
    "googleapis": "^144.0.0",
    "jsdom": "^25.0.1",
    "next": "14.2.5",
    "next-seo": "^6.6.0",
    "nextjs-toploader": "^3.7.15",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241022.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "drizzle-kit": "^0.27.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-next-on-pages": "^1.13.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "vercel": "^37.14.0",
    "wrangler": "^3.84.1"
  }
}
